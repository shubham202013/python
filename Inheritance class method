class person:
    def __init__(self,fname,lname):
        self.firstname=fname
        self.lastname=lname
    def printname(self):
        print(self.firstname,self.lastname,self.fathername)
class student(person):
    def __init__(self,fname,lname,father):
        person. __init__(self,fname,lname)
        self.fathername=father
    def welcom(self):
        print("welcome",self.firstname,self.lastname,self.fathername,"into the class")
x=student("mihir","patel","rameshbhai")
x.welcom()
